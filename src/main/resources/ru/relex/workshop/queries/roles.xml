<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
        
        <entry key="getAllRoles">
        	<![CDATA[
                SELECT
                	id AS id,
                	name AS name,
                	description AS description
                FROM roles
        	]]>
        </entry>

</properties>